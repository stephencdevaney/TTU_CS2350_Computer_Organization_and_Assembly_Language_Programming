 00000060  83 C6 04			add   esi,4		; point to next integer
 00000063  E2 F9			loop  L1		; repeat for array size
 00000065			L2:
 00000065  59				pop   ecx		; restore ECX, ESI
 00000066  5E				pop   esi
 00000067  C3				ret		; sum is in EAX
 00000068			ArraySum ENDP


